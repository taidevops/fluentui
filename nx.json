{
  "npmScope": "tai-ui",
  "implicitDependencies": {
    "package.json": {
      "dependencies": "*",
      "devDependencies": "*"
    }
  },
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/workspace/tasks-runners/default",
      "options": {
        "cacheableOperations": ["build", "test", "lint", "package", "prepare"]
      }
    }
  },
  "workspaceLayout": {
    "libsDir": "packages",
    "appsDir": "apps"
  },
  "affected": {
    "defaultBase": "master"
  },
  "projects": {
    "@tai-ui/a11y-testing": {
      "implicitDependencies": []
    },
    "@tai-ui/eslint-plugin": {
      "implicitDependencies": []
    },
    "@tai-ui/make-styles": {
      "tags": ["vNext", "platform:web"],
      "implicitDependencies": []
    },
    "@tai-ui/react-aria": {
      "tags": ["vNext"],
      "implicitDependencies": []
    },
    "@tai-ui/scripts": {
      "implicitDependencies": []
    },
    "@tai-ui/set-version": {
      "implicitDependencies": []
    },
    "typings": {
      "tags": ["platform:any"]
    },
    "nx-workspace-tools": {
      "tags": []
    }
  }
}
